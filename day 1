int main()
{
	int i,n;
	printf("enter a number: ");
	scanf("%d",&n);
	printf("even num");
	if(n%2==0)
	printf("%d is even", n);
	else
	printf("%d is odd", n);
return 0;
}

#9

#include<stdio.h>
int main(){
  int i , s;
  printf("Enter the Nth term: ");
  scanf("%d",&s);
  int sum =0;
  for( i=1 ; i<s;i++)
    if(i%2 ==0){
        printf("%d ",i);
        printf(" + ");
        sum = sum +i;
    }
    printf("\nThe Sum of N Even Numbers: %d", sum);
    return 0;

  }
	
