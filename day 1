#include<stdio.h>
int main()
{
    int s;
    printf("Enter any Integer: ");
    scanf("%d",&s);
    if(s>= -1){
        printf("%d is a Positive Number ",s);
    }
    else
        printf("%d is a Negative Number",s);
 return 0;
 }
 #15
   #include<stdio.h>
int main(){
  int s , e, sel;
  printf("A = ");
  scanf("%d",&s);
  printf("B = ");
  scanf("%d",&e);
  printf("\n \t \t Before Swapping");
  printf("\nA= %d",s);
  printf("\nB= %d",e);
  printf("\n \t \t After Swapping");
  sel =s;
  s = e;
  e =sel;
  printf("\nA= %d",s);
  printf("\nB= %d",e);
  return 0;
  }
