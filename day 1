#4

#include<stdio.h>
int main()
{
	int i,n;
    int a = 0,b=1;
	int c = a+b;
	printf("enter a  number:    ");
	scanf("%d",&n);
	printf("fibonacci series: %d %d ", a, b);
	for(i=3;i<=n;++i){
	
	printf("%d ",c);
	a=b;
    b=c;
    c=a+b;	
}
return 0;
}

#5
#include<stdio.h>
int main()
{
  int i, s, sum =0;
  printf("Enter the Nth term: ");
  scanf("%d",&s);
  for(i =1 ;i<=s;i++)
    printf("%d + ", i*i*i);
    sum = sum + i*i*i;
    printf("\n Sum of Cubes of N Numbers: %d",sum);
return 0;
  }
	
